<VirtualHost *:81>
    ServerName xn--var-xla.net
    ServerAlias www.xn--var-xla.net aevar.net www.aevar.net
    DocumentRoot /var/www/xn--var-xla.net

    <FilesMatch "\.(ico|jpg|jpeg|gif|png|css|js)$">
        ExpiresActive on  
        ExpiresDefault "access plus 11 days"
    </FilesMatch>

    RewriteEngine On
    # Cool URIs don't change. Ours aren't cool, but at least they don't break.
    RewriteRule ^/2010/10/12/Ready-for-Amsterdam/      /2010/10/12/Moving-to-Amsterdam/         [R=301,L]
</VirtualHost>
