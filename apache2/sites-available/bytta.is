<VirtualHost *:81>
        ServerName bytta.is
	ServerAlias www.bytta.is
        DocumentRoot /var/www/bytta.is/
        Alias /robots.txt /etc/apache2/sites-common/robots.txt/disallow-all.txt

	<Location />
            AuthName "protected"
            AuthType Basic
            AuthUserFile /etc/apache2/passwd/bytta.is
            <Limit GET POST>
                require valid-user
            </Limit>
	</Location>
</VirtualHost>
